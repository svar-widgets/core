{
	"name": "wx-svelte-core",
	"version": "2.0.0-beta-1",
	"description": "SVAR Svelte Core library, a collection of form controls and UI components",
	"productTag": "core",
	"productTrial": false,
	"type": "module",
	"scripts": {
		"build": "vite build",
		"build:dist": "vite build --mode dist",
		"build:tests": "vite build --mode test",
		"lint": "yarn eslint ./demos ./src",
		"start": "vite --open",
		"start:tests": "vite --open=/tests/ --host 0.0.0.0  --port 5100 --mode test",
		"test": "true",
		"test:cypress": "cypress run -P ./ --config \"baseUrl=http://localhost:5100/tests\""
	},
	"svelte": "src/index.js",
	"exports": {
		".": {
			"svelte": "./src/index.js"
		},
		"./package.json": "./package.json"
	},
	"license": "MIT",
	"dependencies": {
		"wx-core-locales": "2.0.0-beta-1",
		"wx-lib-dom": "0.6.0"
	},
	"files": [
		"src",
		"readme.md",
		"whatsnew.md",
		"license.txt"
	],
	"keywords": [
		"svelte",
		"svelte library",
		"svelte ui components",
		"svelte-ui-library",
		"component-library",
		"components library",
		"ui library",
		"ui-library",
		"ui components",
		"ui components",
		"svelte ui library",
		"svar widgets"
	]
}
